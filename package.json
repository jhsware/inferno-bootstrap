{
  "name": "inferno-bootstrap",
  "version": "6.0.1",
  "description": "Bootstrap as Inferno components, adapted from Reactstrap",
  "main": "dist/cjs/index.js",
  "umd:main": "dist/umd/index.js",
  "module": "lib/index.js",
  "esnext": "src",
  "sideEffects": false,
  "scripts": {
    "test": "npm run build:clean && cross-env NODE_ENV=development npm run build:es && cross-env NODE_ENV=development npm run build:dist && cross-env NODE_ENV=production npm run build:dist:prod && jest --forceExit",
    "test-browser": "npm run build:clean-test && npm run build-test && node test/browser/server",
    "prepublish": "npm run build",
    "postversion": "git push && git push --tags",
    "build": "npm run build:clean && cross-env NODE_ENV=development npm run build:es && cross-env NODE_ENV=development npm run build:dist && cross-env NODE_ENV=production npm run build:dist:prod && cross-env NODE_ENV=production npm run build:legacy",
    "build:clean": "rm -rf dist/ && rm -rf lib/",
    "build:es": "babel src --out-dir lib",
    "build:legacy": "babel src --out-dir dist",
    "build:dist": "rollup -c",
    "build:dist:prod": "rollup -c",
    "build:clean-test": "rm -rf ./test/dist",
    "build-test": "cross-env NODE_ENV=test node_modules/.bin/webpack --config ./test/browser/webpack.config",
    "build-test-dev": "cross-env NODE_ENV=test node_modules/.bin/webpack --watch --config ./test/browser/webpack.config",
    "lint": "eslint --ext .js,.jsx src/ example/"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jhsware/inferno-bootstrap.git"
  },
  "keywords": [
    "bootstrap",
    "inferno"
  ],
  "author": "Sebastian Ware <sebastian@urbantalk.se> (https://github.com/jhsware)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jhsware/inferno-bootstrap/issues"
  },
  "homepage": "https://github.com/jhsware/inferno-bootstrap#readme",
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "bootstrap": "^4.1.3",
    "classnames": "^2.2.6",
    "inferno": "^6.0.2",
    "inferno-animation": "^6.0.0",
    "inferno-create-element": "^6.0.2",
    "inferno-extras": "^6.0.3",
    "inferno-popper": "^6.0.1",
    "inferno-shared": "^6.0.2",
    "inferno-vnode-flags": "^6.0.2",
    "lodash.isfunction": "^3.0.9",
    "lodash.isobject": "^3.0.2",
    "lodash.tonumber": "^4.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-syntax-jsx": "^7.0.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "7.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-inferno": "6.0.0",
    "babel-preset-inferno-app": "^8.0.1",
    "bootstrap": "^4.1.3",
    "clean-webpack-plugin": "^0.1.19",
    "component-registry": "1.0.0-rc.7",
    "create-inferno-app": "^7.0.5",
    "cross-env": "^5.2.0",
    "express": "^4.16.2",
    "inferno-devtools": "^6.0.2",
    "inferno-router": "^6.0.1",
    "inferno-test-utils": "^6.0.2",
    "isomorphic-schema": "1.0.0-beta.7",
    "jest": "^23.6.0",
    "jsdom": "^11.6.2",
    "prismjs": "^1.9.0",
    "regenerator-runtime": "^0.12.1",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-babel-minify": "^6.1.1",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-replace": "^2.1.0",
    "sinon": "^7.0.0",
    "webpack": "^3.11.0"
  }
}
