{
  "name": "inferno-bootstrap",
  "version": "5.0.2",
  "description": "Bootstrap as Inferno components, adapted from Reactstrap",
  "main": "dist/cjs/index.js",
  "umd:main": "dist/umd/index.js",
  "module": "lib/index.js",
  "esnext": "src",
  "sideEffects": false,
  "scripts": {
    "test": "jest",
    "test-browser": "npm run build-test && node test/browser/server",
    "prepublish": "npm run build",
    "postversion": "git push && git push --tags",

    "__build": "rm -rf ./dist && cross-env NODE_ENV=production babel lib --out-dir dist --source-maps",
    "build": "npm run build:clean && npm run build:es && npm run build:dist && npm run build:dist:prod",
    "build:clean": "rm -rf dist/ && rm -rf lib/",
    "build:es": "cross-env NODE_ENV=development babel src --out-dir lib",
    "build:dist": "cross-env NODE_ENV=development rollup -c",
    "build:dist:prod": "cross-env NODE_ENV=production rollup -c",
    
    "build-test": "rm -rf ./test/dist && cross-env NODE_ENV=test node_modules/.bin/webpack --config ./test/browser/webpack.config",
    "build-test-dev": "rm -rf ./test/dist && cross-env NODE_ENV=development node_modules/.bin/webpack --watch --config ./test/browser/webpack.config",

    "start": "cross-env BABEL_ENV=webpack webpack-dev-server --config ./webpack.dev.config.js --watch",
    "lint": "eslint --ext .js,.jsx src/ example/"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jhsware/inferno-bootstrap.git"
  },
  "keywords": [
    "bootstrap",
    "inferno"
  ],
  "author": "Sebastian Ware <sebastian@urbantalk.se> (https://github.com/jhsware)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jhsware/inferno-bootstrap/issues"
  },
  "homepage": "https://github.com/jhsware/inferno-bootstrap#readme",
  "dependencies": {
    "classnames": "^2.2.6",
    "inferno": "^5.0.1",
    "inferno-animation": "^5.0.0",
    "inferno-create-element": "^5.0.1",
    "inferno-popper": "^5.0.0",
    "inferno-shared": "^5.0.1",
    "inferno-vnode-flags": "^5.0.1",
    "lodash.isfunction": "^3.0.9",
    "lodash.isobject": "^3.0.2",
    "lodash.tonumber": "^4.0.3"
  },
  "peerDependencies": {
    "bootstrap": "^4.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^6.4.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-inferno": "^5.0.1",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-inferno-app": "^7.1.0",
    "babel-preset-stage-2": "^6.24.1",

    "bootstrap": "^4.1.1",

    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.0.5",

    "express": "^4.16.2",
    "inferno-devtools": "^5.0.1",
    "inferno-router": "^5.0.1",
    "inferno-test-utils": "^5.0.1",
    "jest": "^22.4.2",
    "jsdom": "^11.6.2",
    "prismjs": "^1.9.0",

    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-babel-minify": "^4.0.0",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-globals": "^1.2.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",

    "sinon": "^2.3.8",
    "webpack": "^2.7.0"
  }
}
